<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#131313"
    tools:context=".ui.slideshow.SlideshowFragment">

    <TextView
        android:id="@+id/text_slideshow"
        android:layout_width="335dp"
        android:layout_height="46dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textColor="#F3ECEC"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.018" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="291dp"
        android:layout_height="69dp"
        android:layout_marginStart="60dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/graphentrie"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/Notification"
        android:layout_width="42dp"
        android:layout_height="37dp"
        android:background="@color/yellow"
        android:src="@drawable/ic_baseline_add_alert_24"
        android:text="@string/slideshow_text1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="259dp"
        android:layout_height="40dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="84dp"
        android:text="I thought i had it under control. I regret not acting sooner. Now it is all lost."
        android:textColor="#E80909"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="251dp"
        android:layout_height="45dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="136dp"
        android:text="I feel sick. It is non ending. How do i find myself. "
        android:textColor="#8C3131"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="263dp"
        android:layout_height="50dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="180dp"
        android:text="This process is taking too long. I am feeling very vulnerable. I need help."
        android:textColor="#A8543A"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="270dp"
        android:layout_height="78dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="128dp"
        android:text="Day 4 on this process. Writring down what i feel everyday as helped me deal with various emotions recently. I think i am being pulled back from the edge."
        android:textColor="#F68F6F"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="260dp"
        android:layout_height="51dp"
        android:layout_marginStart="92dp"
        android:layout_marginBottom="432dp"
        android:text="I am getting used to this. Today was a good day. I am happy because i found some closure. "
        android:textColor="#EDB95D"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="245dp"
        android:layout_height="66dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="460dp"
        android:text="I am at day 6 of this week. Looking back to when i started keeping track of the days emotions. I see what am doing wrong."
        android:textColor="#D6D613"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="253dp"
        android:layout_height="67dp"
        android:layout_marginStart="140dp"
        android:layout_marginBottom="256dp"
        android:text="I figured out a way to help myself heal. From now on instaed of meditating on the problem I will actually deal with the problem"
        android:textColor="#6DB51A"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="358dp"
        android:layout_height="55dp"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="96dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Was recording your emotions helpful."
        android:textColor="@color/colorPrimary"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="54dp"
        android:layout_height="43dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="136dp"
        android:src="@drawable/angry"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="51dp"
        android:layout_height="49dp"
        android:layout_marginStart="340dp"
        android:layout_marginBottom="564dp"
        android:src="@drawable/happybutton"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="89dp"
        android:layout_height="62dp"
        android:layout_marginStart="304dp"
        android:layout_marginBottom="168dp"
        android:src="@drawable/customer_rev"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>