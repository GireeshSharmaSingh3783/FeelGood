<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.aceapps.it.feelgood">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/feelgood"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/feelgood"
        android:supportsRtl="true"
        android:theme="@style/Theme.FeelGood">
        <activity
            android:name=".ui.ReviewScreen"
            android:exported="false" />
        <activity
            android:name=".MentalDistortions"
            android:exported="false" />
        <activity
            android:name=".SignUpActivity"
            android:exported="true" />
        <activity
            android:name=".LoginActivity"
            android:exported="true" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.FeelGood.NoActionBar" />
    </application>

</manifest>